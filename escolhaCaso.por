programa
{
	
	funcao inicio()
	{
		inteiro mes

		escreva("Informe qual o mês: \n")
		leia(mes)
		
		escolha(mes) {
			caso 1:
				escreva("Janeiro")
				pare
			caso 2:
				escreva("Fevereiro")
				pare
			caso 3:
				escreva("Março")
				pare
			caso 4:
				escreva("Abril")
				pare
			caso contrario:
				escreva("Inválido")
				
		}

		
		
		
		
	}
}
